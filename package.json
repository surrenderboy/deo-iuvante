{
  "name": "shri-boilerplate",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8080/",
  "dependencies": {
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addon-info": "^3.3.15",
    "@storybook/addon-knobs": "^3.3.15",
    "@storybook/addon-links": "^3.3.15",
    "@storybook/addons": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "concurrently": "^3.5.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.2",
    "faker": "^4.1.0",
    "mongod": "^2.0.0",
    "mongodb": "^3.0.3",
    "nodemon": "^1.15.1",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "2.0.0-next.b2fd8db8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "socket.io-cookie-parser": "^1.0.0",
    "uuid": "^3.2.1"
  },
  "scripts": {
    "start": "concurrently \"npm run client\" \"npm run server\"",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:integration": "gemini test",
    "enzyme": "jest",
    "eject": "react-scripts eject",
    "client": "react-scripts start",
    "server": "nodemon -w server.js -w src/server server.js",
    "server-prod": "node server.js",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public -o docs",
    "lint": "eslint --ext .js --ext .jsx ./src",
    "lint:fix": "npm run lint -- --fix",
    "lint:staged": "./lint-staged.sh"
  },
  "devDependencies": {
    "babel-core": "7.0.0-bridge.0",
    "babel-preset-react-app": "^3.1.1",
    "chai": "^4.1.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "gemini": "^5.6.2",
    "jest-css-modules": "^1.1.0",
    "jest-enzyme": "^6.0.0",
    "mocha": "^5.1.0",
    "pre-commit": "^1.2.2",
    "react-test-renderer": "^16.3.1",
    "babel-eslint": "~8.2"
  },
  "browserslist": {
    "development": [
      "last 2 versions"
    ],
    "production": [
      "last 2 versions"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "pre-commit": "lint:staged"
